{
  "name": "react-static-starter-kit",
  "version": "3.1.0",
  "description": "Experimental React static prerendered app starter kit",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && NODE_ENV=production webpack --colors --profile --config config/build.conf.ts --display-error-details",
    "test": "echo \"No tests yet\"",
    "dev": "NODE_ENV=development webpack-dev-server --display-error-details --config config/build.conf.ts --open",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "tslint \"src/**/*.ts\" \"src/**/*.tsx\" \"config/**/*.ts\" \"config/**/*.tsx\" --config tslint.json --project tsconfig.json",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint \"src/**/*.tsx\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^4.4.1",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.5.8",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "@types/react-helmet": "^5.0.7",
    "@types/react-intl": "^2.3.9",
    "@types/react-redux": "^6.0.6",
    "@types/react-router-config": "^1.0.8",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-transition-group": "^2.0.13",
    "@types/superagent": "^3.8.3",
    "@types/webpack": "^4.4.9",
    "@types/webpack-bundle-analyzer": "^2.9.2",
    "@types/webpack-env": "^1.13.6",
    "copy-webpack-plugin": "^4.5.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.10",
    "prerender-spa-plugin": "^3.2.1",
    "raw-loader": "^0.5.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.4.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2",
    "styled-components": "3.4.2",
    "styled-normalize": "^8.0.0",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.2",
    "superagent": "^3.8.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {}
}
