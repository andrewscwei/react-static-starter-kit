{
  "name": "react-static-starter-kit",
  "version": "16.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "serve build -s -p ${PORT:-8080}",
    "clean": "rimraf build",
    "prebuild": "npm run lint && npm run clean",
    "build": "BUILD_NUMBER=$(git rev-parse --short HEAD) webpack --profile --config config/build.conf.ts",
    "dev": "cross-env NODE_ENV=development webpack-cli serve --mode development --config config/build.conf.ts",
    "test": "playwright test",
    "test:unit": "jest --config=config/test.conf.ts src/$npm_config_files",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "eslint",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint src/**/*.css",
    "lint:css:fix": "npm run lint:css -- --fix",
    "scan": "dotenv -- sh -c 'sonar-scanner -Dsonar.projectKey=$SONAR_PROJECT_KEY -Dsonar.sources=. -Dsonar.host.url=$SONAR_HOST_URL -Dsonar.token=$SONAR_TOKEN'"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@playwright/test": "^1.45.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin": "^2.3.0",
    "@testing-library/react": "^16.0.0",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/debug": "^4.1.12",
    "@types/html-webpack-plugin": "^3.2.9",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/sprintf-js": "^1.1.4",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@types/webpack-env": "^1.18.5",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "debug": "^4.3.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "esbuild-loader": "^4.2.0",
    "eslint": "^9.6.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss": "^8.4.39",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.5.15",
    "react-refresh": "^0.14.2",
    "rimraf": "^5.0.7",
    "semantic-release": "^24.0.0",
    "serve": "^14.2.3",
    "style-loader": "^4.0.0",
    "stylelint": "^16.6.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0-alpha.10",
    "webpack": "^5.92.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "whatwg-fetch": "^3.6.20",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.24.0",
    "react-router-dom": "^6.24.0",
    "sprintf-js": "^1.1.3"
  }
}
