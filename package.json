{
  "name": "react-static-starter-kit",
  "version": "10.0.0",
  "description": "React static app starter kit",
  "homepage": "https://react-static-starter-kit.netlify.app",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "http-server build --proxy http://localhost:${PORT:-8080}?",
    "clean": "rimraf build",
    "prebuild": "npm run lint",
    "build": "npm run clean && BUILD_NUMBER=$(git rev-parse --short HEAD).$(git rev-list --count HEAD) webpack --profile --config config/build.conf.ts",
    "test": "jest -b --colors --passWithNoTests --testEnvironment=jsdom tests/$npm_config_files",
    "test:ts": "jest -b --colors --passWithNoTests --testEnvironment=jsdom src/$npm_config_files",
    "dev": "cross-env NODE_ENV=development webpack-cli serve --mode development --config config/build.conf.ts",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "eslint --ext .ts --ext .tsx src config",
    "lint:ts:fix": "npm run lint -- --fix",
    "lint:css": "stylelint src/**/*.css",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/core-js": "^2.5.5",
    "@types/debug": "^4.1.7",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.23",
    "@types/node-polyglot": "^2.4.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.2",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.13",
    "debug": "^4.3.4",
    "dotenv": "^16.0.2",
    "eslint": "^8.24.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.16",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.2",
    "react-refresh": "^0.14.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.12.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "abortcontroller-polyfill": "^1.7.3",
    "classnames": "^2.3.2",
    "core-js": "^3.25.3",
    "node-polyglot": "^2.4.2",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.4.1",
    "react-router-dom": "^6.4.1",
    "whatwg-fetch": "^3.6.2"
  }
}
