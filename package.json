{
  "name": "react-static-starter-kit",
  "version": "17.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "vite preview --port ${PORT:-8080}",
    "clean": "rimraf build",
    "prebuild": "npm run lint && npm run clean",
    "build": "BUILD_NUMBER=$(git rev-parse --short HEAD) vite build",
    "dev": "vite dev",
    "test": "playwright test",
    "test:unit": "vitest run --coverage",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "eslint",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint src/**/*.css",
    "lint:css:fix": "npm run lint:css -- --fix",
    "scan": "dotenv -- sh -c 'sonar-scanner -Dsonar.projectKey=$SONAR_PROJECT_KEY -Dsonar.sources=. -Dsonar.host.url=$SONAR_HOST_URL -Dsonar.token=$SONAR_TOKEN'"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@playwright/test": "^1.45.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin": "^2.3.0",
    "@testing-library/react": "^16.0.0",
    "@types/debug": "^4.1.12",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/node": "^20.14.11",
    "@types/postcss-import": "^14.0.3",
    "@types/postcss-preset-env": "^8.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/sprintf-js": "^1.1.4",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.3",
    "debug": "^4.3.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.7.0",
    "happy-dom": "^14.12.3",
    "html-minifier-terser": "^7.2.0",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^9.6.0",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.0.0",
    "stylelint": "^16.7.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0-alpha.44",
    "vite": "^5.3.4",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^2.0.3"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.25.1",
    "react-router-dom": "^6.25.1",
    "sprintf-js": "^1.1.3"
  }
}
