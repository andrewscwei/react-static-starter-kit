{
  "name": "react-static-starter-kit",
  "version": "10.0.0",
  "description": "React static app starter kit",
  "homepage": "https://react-static-starter-kit.netlify.app",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "prebuild": "npm run lint",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --profile --config config/build.conf.ts",
    "test": "jest -b --colors --passWithNoTests --testEnvironment=jsdom tests/$npm_config_files",
    "test:ts": "jest -b --colors --passWithNoTests --testEnvironment=jsdom src/$npm_config_files",
    "dev": "cross-env NODE_ENV=development webpack-cli serve --mode development --config config/build.conf.ts",
    "lint": "eslint --ext .ts --ext .tsx src config",
    "lint:fix": "npm run lint -- --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/core-js": "^2.5.5",
    "@types/debug": "^4.1.7",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/jest": "^29.0.3",
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.7.18",
    "@types/node-polyglot": "^2.4.2",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/require-dir": "^1.0.2",
    "@types/styled-components": "^5.1.26",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.2",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.0.3",
    "raw-loader": "^4.0.2",
    "react-refresh": "^0.14.0",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "speed-measure-webpack-plugin": "^1.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "abortcontroller-polyfill": "^1.7.3",
    "core-js": "^3.25.2",
    "lodash": "^4.17.21",
    "node-polyglot": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router": "^6.4.1",
    "react-router-dom": "^6.4.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.6",
    "styled-components": "5.3.5",
    "styled-normalize": "^8.0.7",
    "whatwg-fetch": "^3.6.2"
  }
}
