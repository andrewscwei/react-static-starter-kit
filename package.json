{
  "name": "react-static-starter-kit",
  "version": "10.2.0",
  "description": "React static app starter kit",
  "homepage": "https://react-static-starter-kit.netlify.app",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "http-server build --proxy http://localhost:${PORT:-8080}?",
    "clean": "rimraf build",
    "prebuild": "npm run lint",
    "build": "npm run clean && BUILD_NUMBER=$(git rev-parse --short HEAD).$(git rev-list --count HEAD) webpack --profile --config config/build.conf.ts",
    "dev": "cross-env NODE_ENV=development webpack-cli serve --mode development --config config/build.conf.ts",
    "test": "jest --config=config/test.conf.ts tests/$npm_config_files",
    "test:ts": "jest --silent --config=config/test.conf.ts src/$npm_config_files",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "eslint --ext .ts --ext .tsx src config tests",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint src/**/*.css",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@testing-library/react": "^14.0.0",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/debug": "^4.1.7",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/node-polyglot": "^2.4.2",
    "@types/object-hash": "^3.0.2",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@types/webpack": "^5.28.1",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.1",
    "http-server": "^14.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "^8.4.22",
    "postcss-loader": "^7.2.4",
    "postcss-preset-env": "^8.3.2",
    "react-refresh": "^0.14.0",
    "rimraf": "^5.0.0",
    "style-loader": "^3.3.2",
    "stylelint": "^15.5.0",
    "terser-webpack-plugin": "^5.3.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "webpack": "^5.79.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.3",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "core-js": "^3.30.1",
    "node-polyglot": "^2.5.0",
    "normalize.css": "^8.0.1",
    "object-hash": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0"
  }
}
