    "@types/debug": "^4.1.12",
    "@types/ejs": "^3.1.5",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/sprintf-js": "^1.1.4",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^17.2.1",
    "ejs": "^3.1.10",
