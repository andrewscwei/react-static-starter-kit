{
  "name": "react-static-starter-kit",
  "version": "16.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "http-server build -a localhost -P http://localhost:${PORT:-8080}? -p ${PORT:-8080}",
    "clean": "rimraf build",
    "prebuild": "npm run lint && npm run clean",
    "build": "BUILD_NUMBER=$(git rev-parse --short HEAD) webpack --profile --config config/build.conf.ts",
    "dev": "cross-env NODE_ENV=development webpack-cli serve --mode development --config config/build.conf.ts",
    "test": "jest --config=config/test.conf.ts tests/$npm_config_files",
    "test:unit": "jest --config=config/test.conf.ts src/$npm_config_files",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "eslint --ext .ts --ext .tsx --ignore-pattern *.d.ts config lib src tests",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint src/**/*.css",
    "lint:css:fix": "npm run lint:css -- --fix",
    "scan": "dotenv -- sh -c 'sonar-scanner -Dsonar.projectKey=$SONAR_PROJECT_KEY -Dsonar.sources=. -Dsonar.host.url=$SONAR_HOST_URL -Dsonar.token=$SONAR_TOKEN'"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@testing-library/react": "^14.1.2",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/debug": "^4.1.12",
    "@types/html-webpack-plugin": "^3.2.9",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.7",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/sprintf-js": "^1.1.4",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.6.3",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.9.1",
    "debug": "^4.3.4",
    "dotenv": "^16.4.1",
    "dotenv-cli": "^7.4.1",
    "esbuild-loader": "^4.0.3",
    "eslint": "^8.56.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.6.0",
    "http-server": "^14.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.7",
    "postcss": "^8.4.33",
    "postcss-import": "^16.0.0",
    "postcss-loader": "^8.0.0",
    "postcss-preset-env": "^9.3.0",
    "react-refresh": "^0.14.0",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.4",
    "stylelint": "^16.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "whatwg-fetch": "^3.6.20",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.21.3",
    "react-router-dom": "^6.21.3",
    "sprintf-js": "^1.1.3"
  }
}
