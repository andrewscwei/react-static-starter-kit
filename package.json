{
  "name": "react-static-starter-kit",
  "version": "6.4.0",
  "description": "Experimental React static prerendered app starter kit",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && NODE_ENV=production webpack --colors --profile --config config/build.conf.ts --display-error-details",
    "postbuild": "react-snap && ts-node config/build.post",
    "test": "echo \"No tests yet\"",
    "dev": "NODE_ENV=development webpack-dev-server --config config/build.conf.ts",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "eslint --ext .ts --ext .tsx src",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint \"src/**/*.tsx\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "reactSnap": {
    "source": "build",
    "include": [
      "/preview"
    ],
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/chalk": "^2.2.0",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash": "^4.14.159",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.6.0",
    "@types/node-polyglot": "^2.4.1",
    "@types/prismic-dom": "^2.1.1",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-hot-loader": "^4.1.1",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-transition-group": "^4.4.0",
    "@types/require-dir": "^1.0.1",
    "@types/styled-components": "^5.1.2",
    "@types/webpack": "^4.41.21",
    "@types/webpack-bundle-analyzer": "^3.8.0",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "raw-loader": "^4.0.1",
    "react-hot-loader": "^4.12.21",
    "react-snap": "^1.23.0",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-node": "^8.10.2",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "worker-loader": "^3.0.1"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "node-polyglot": "^2.4.0",
    "prismic-dom": "^2.2.3",
    "prismic-javascript": "^3.0.2",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "5.1.1",
    "styled-normalize": "^8.0.7"
  }
}
