{
  "name": "react-static-starter-kit",
  "version": "3.4.0",
  "description": "Experimental React static prerendered app starter kit",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && NODE_ENV=production node -r @babel/register ./node_modules/.bin/webpack --colors --profile --config config/build.conf.js --display-error-details",
    "test": "echo \"No tests yet\"",
    "dev": "NODE_ENV=development node -r @babel/register ./node_modules/.bin/webpack-dev-server --display-error-details --hot --config config/build.conf.js --open",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint \"src/**/*.jsx\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-styled-components": "^1.7.1",
    "copy-webpack-plugin": "^4.5.2",
    "dotenv": "^6.0.0",
    "eslint": "^5.6.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "lodash": "^4.17.11",
    "prerender-spa-plugin": "^3.4.0",
    "prop-types": "^15.6.2",
    "raw-loader": "^0.5.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.6.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.4.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "require-dir": "^1.0.0",
    "rimraf": "^2.6.2",
    "styled-components": "3.4.9",
    "styled-normalize": "^8.0.1",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.2",
    "superagent": "^4.0.0-beta.5",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.1",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "dependencies": {}
}
