{
  "name": "react-static-starter-kit",
  "version": "5.0.0",
  "description": "Experimental React static prerendered app starter kit",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && NODE_ENV=production webpack --colors --profile --config config/build.conf.ts --display-error-details",
    "postbuild": "react-snap",
    "test": "echo \"No tests yet\"",
    "dev": "NODE_ENV=development webpack-dev-server --display-error-details --hot --config config/build.conf.ts",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "tslint \"src/**/*.ts\" \"src/**/*.tsx\" \"config/**/*.ts\" \"config/**/*.tsx\" --config tslint.json --project tsconfig.json",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint \"src/**/*.tsx\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "reactSnap": {
    "source": "build",
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@hot-loader/react-dom": "^16.8.6",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/lodash": "^4.14.134",
    "@types/node": "^12.0.7",
    "@types/node-polyglot": "^0.4.33",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^7.0.9",
    "@types/react-router-config": "^5.0.0",
    "@types/react-router-dom": "^4.3.3",
    "@types/react-transition-group": "^2.9.2",
    "@types/require-dir": "^1.0.0",
    "@types/styled-components": "4.1.8",
    "@types/webpack": "^4.4.32",
    "@types/webpack-bundle-analyzer": "^2.13.1",
    "@types/webpack-env": "^1.13.9",
    "abortcontroller-polyfill": "^1.3.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.0",
    "copy-webpack-plugin": "^5.0.3",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "file-loader": "^4.0.0",
    "hoist-non-react-statics": "^3.3.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^5.0.0",
    "lodash": "^4.17.11",
    "node-polyglot": "^2.3.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.11.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-snap": "^1.23.0",
    "react-transition-group": "^4.1.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "require-dir": "^1.2.0",
    "rimraf": "^2.6.3",
    "styled-components": "4.3.1",
    "styled-normalize": "^8.0.6",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1",
    "url-loader": "^2.0.0",
    "webpack": "^4.33.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "whatwg-fetch": "^3.0.0",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {}
}
