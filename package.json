{
  "name": "react-static-starter-kit",
  "version": "8.0.0",
  "description": "Experimental React static prerendered app starter kit",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --profile --config config/build.conf.ts",
    "postbuild": "react-snap && ts-node config/build.post",
    "test": "echo \"No tests yet\"",
    "dev": "cross-env NODE_ENV=development webpack-cli serve --mode development --config config/build.conf.ts",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "eslint --ext .ts --ext .tsx src config",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "reactSnap": {
    "source": "build",
    "include": [
      "/404.html"
    ],
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.4",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/core-js": "^2.5.5",
    "@types/debug": "^4.1.7",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.11.8",
    "@types/node-polyglot": "^2.4.2",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.20",
    "@types/react-transition-group": "^4.4.4",
    "@types/require-dir": "^1.0.2",
    "@types/styled-components": "^5.1.15",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^1.13.3",
    "chalk": "^4.1.2",
    "copy-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "eslint": "^8.2.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.0.1",
    "raw-loader": "^4.0.2",
    "react-refresh": "^0.10.0",
    "react-snap": "^1.23.0",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "speed-measure-webpack-plugin": "^1.5.0",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "abortcontroller-polyfill": "^1.7.3",
    "core-js": "^3.19.1",
    "lodash": "^4.17.21",
    "node-polyglot": "^2.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.0",
    "reselect": "^4.1.4",
    "styled-components": "5.3.3",
    "styled-normalize": "^8.0.7",
    "whatwg-fetch": "^3.6.2"
  }
}
