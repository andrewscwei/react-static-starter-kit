{
  "name": "react-static-starter-kit",
  "version": "7.2.0",
  "description": "Experimental React static prerendered app starter kit",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && NODE_ENV=production webpack --profile --config config/build.conf.ts",
    "postbuild": "react-snap && ts-node config/build.post",
    "test": "echo \"No tests yet\"",
    "dev": "NODE_ENV=development webpack-cli serve --mode development --config config/build.conf.ts",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "eslint --ext .ts --ext .tsx src config",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint \"src/**/*.tsx\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "reactSnap": {
    "source": "build",
    "include": [
      "/404.html"
    ],
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/copy-webpack-plugin": "^6.3.0",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.12",
    "@types/node-polyglot": "^2.4.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.12",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-transition-group": "^4.4.0",
    "@types/require-dir": "^1.0.1",
    "@types/styled-components": "^5.1.5",
    "@types/webpack": "^4.41.25",
    "@types/webpack-bundle-analyzer": "^3.9.0",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^7.0.0",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "image-webpack-loader": "^7.0.1",
    "raw-loader": "^4.0.2",
    "react-refresh": "^0.9.0",
    "react-snap": "^1.23.0",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-node": "^9.1.1",
    "type-fest": "^0.20.2",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.10.1",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "worker-loader": "^3.0.6"
  },
  "dependencies": {
    "abortcontroller-polyfill": "^1.5.0",
    "core-js": "^3.8.1",
    "lodash": "^4.17.20",
    "node-polyglot": "^2.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "5.2.1",
    "styled-normalize": "^8.0.7",
    "whatwg-fetch": "^3.5.0"
  }
}
