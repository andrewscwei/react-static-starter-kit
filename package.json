{
  "name": "react-static-starter-kit",
  "version": "2.0.0",
  "description": "Experimental React static prerendered app starter kit",
  "keywords": [
    "react",
    "static",
    "prerender",
    "styled",
    "components",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && NODE_ENV=production webpack --colors --profile --config config/build.conf.ts --display-error-details && react-snapshot",
    "test": "echo \"No tests yet\"",
    "dev": "NODE_ENV=development webpack-dev-server --display-error-details --config config/build.conf.ts --open",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "tslint \"src/**/*.ts\" \"src/**/*.tsx\" \"config/**/*.ts\" \"config/**/*.tsx\" --config tslint.json --project tsconfig.json",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint \"src/**/*.tsx\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^4.4.1",
    "@types/html-webpack-plugin": "^2.30.3",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-intl": "^2.3.8",
    "@types/react-redux": "^6.0.0",
    "@types/react-router-config": "^1.0.6",
    "@types/react-router-dom": "^4.2.6",
    "@types/stylelint-webpack-plugin": "^0.1.2",
    "@types/webpack": "^4.4.0",
    "@types/webpack-bundle-analyzer": "^2.9.2",
    "@types/webpack-env": "^1.13.6",
    "axios": "^0.18.0",
    "copy-webpack-plugin": "^4.5.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-snapshot": "^1.3.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "styled-components": "3.0.1",
    "styled-normalize": "^4.0.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "ts-loader": "^4.3.0",
    "ts-node": "^6.0.5",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  }
}
