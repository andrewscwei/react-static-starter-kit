{
  "name": "react-static-starter-kit",
  "version": "5.6.0",
  "description": "Experimental React static prerendered app starter kit",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && NODE_ENV=production webpack --colors --profile --config config/build.conf.ts --display-error-details",
    "postbuild": "react-snap",
    "test": "echo \"No tests yet\"",
    "dev": "NODE_ENV=development webpack-dev-server --config config/build.conf.ts",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "tslint \"src/**/*.ts\" \"src/**/*.tsx\" \"config/**/*.ts\" \"config/**/*.tsx\" --config tslint.json --project tsconfig.json",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint \"src/**/*.tsx\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "reactSnap": {
    "source": "build",
    "include": [
      "/404.html"
    ],
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@hot-loader/react-dom": "^16.9.0",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.7.5",
    "@types/node-polyglot": "^0.4.34",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-config": "^5.0.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-transition-group": "^4.2.2",
    "@types/require-dir": "^1.0.0",
    "@types/styled-components": "^4.1.19",
    "@types/webpack": "^4.39.1",
    "@types/webpack-bundle-analyzer": "^2.13.2",
    "@types/webpack-env": "^1.14.0",
    "abortcontroller-polyfill": "^1.3.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "copy-webpack-plugin": "^5.0.4",
    "core-js": "^3.2.1",
    "debug": "^4.1.1",
    "dotenv": "^8.1.0",
    "file-loader": "^4.2.0",
    "hoist-non-react-statics": "^3.3.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "lodash": "^4.17.15",
    "node-polyglot": "^2.4.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^3.1.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.13",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-snap": "^1.23.0",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.0",
    "styled-components": "4.3.2",
    "styled-normalize": "^8.0.6",
    "stylelint": "^11.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.40.2",
    "webpack-bundle-analyzer": "^3.5.0",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "whatwg-fetch": "^3.0.0",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {}
}
