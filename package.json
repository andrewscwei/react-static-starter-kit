{
  "name": "react-static-starter-kit",
  "version": "6.0.1",
  "description": "Experimental React static prerendered app starter kit",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && NODE_ENV=production node -r @babel/register ./node_modules/.bin/webpack --colors --profile --config config/build.conf.js --display-error-details",
    "postbuild": "react-snap && node config/build.post.mjs",
    "test": "echo \"No tests yet\"",
    "dev": "NODE_ENV=development node -r @babel/register ./node_modules/.bin/webpack-dev-server --config config/build.conf.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint . --ignore-path .gitignore --ext .jsx --ext .js",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint \"src/**/*.jsx\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "reactSnap": {
    "source": "build",
    "puppeteer": {
      "networkIdleTimeout": 5000,
      "waitUntil": "networkidle"
    },
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@hot-loader/react-dom": "^16.13.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "chalk": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.3",
    "image-webpack-loader": "^6.0.0",
    "raw-loader": "^4.0.0",
    "react-hot-loader": "^4.12.20",
    "react-snap": "^1.23.0",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.2.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "abortcontroller-polyfill": "^1.4.0",
    "core-js": "^3.6.4",
    "lodash": "^4.17.15",
    "node-polyglot": "^2.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "5.0.1",
    "styled-normalize": "^8.0.7",
    "whatwg-fetch": "^3.0.0"
  }
}
