{
  "name": "react-static-starter-kit",
  "version": "4.0.0",
  "description": "Experimental React static prerendered app starter kit",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && NODE_ENV=production node -r @babel/register ./node_modules/.bin/webpack --colors --profile --config config/build.conf.js --display-error-details",
    "test": "echo \"No tests yet\"",
    "dev": "NODE_ENV=development node -r @babel/register ./node_modules/.bin/webpack-dev-server --display-error-details --hot --config config/build.conf.js --open",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint . --ignore-path .gitignore --ext .jsx --ext .js",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint \"src/**/*.jsx\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "@hot-loader/react-dom": "^16.8.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "copy-webpack-plugin": "^5.0.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.14.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "lodash": "^4.17.11",
    "prerender-spa-plugin": "^3.4.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^1.0.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.7.1",
    "react-intl": "^2.8.0",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.6.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "require-dir": "^1.2.0",
    "rimraf": "^2.6.3",
    "styled-components": "4.1.3",
    "styled-normalize": "^8.0.6",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2",
    "superagent": "^4.1.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.5",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {}
}
