{
  "name": "react-static-starter-kit",
  "version": "5.8.1",
  "description": "Experimental React static prerendered app starter kit",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && NODE_ENV=production webpack --colors --profile --config config/build.conf.ts --display-error-details",
    "postbuild": "react-snap",
    "test": "echo \"No tests yet\"",
    "dev": "NODE_ENV=development webpack-dev-server --config config/build.conf.ts",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "tslint \"src/**/*.ts\" \"src/**/*.tsx\" \"config/**/*.ts\" \"config/**/*.tsx\" --config tslint.json --project tsconfig.json",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint \"src/**/*.tsx\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "reactSnap": {
    "source": "build",
    "include": [
      "/404.html"
    ],
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@hot-loader/react-dom": "^16.10.2",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.11.7",
    "@types/node-polyglot": "^0.4.34",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-transition-group": "^4.2.3",
    "@types/require-dir": "^1.0.0",
    "@types/styled-components": "^4.1.20",
    "@types/webpack": "^4.39.5",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "@types/webpack-env": "^1.14.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "copy-webpack-plugin": "^5.0.4",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "raw-loader": "^3.1.0",
    "react-hot-loader": "^4.12.15",
    "react-snap": "^1.23.0",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "abortcontroller-polyfill": "^1.3.0",
    "core-js": "^3.3.5",
    "lodash": "^4.17.15",
    "node-polyglot": "^2.4.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "styled-components": "4.4.0",
    "styled-normalize": "^8.0.6",
    "whatwg-fetch": "^3.0.0"
  }
}
