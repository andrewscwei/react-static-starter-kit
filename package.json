{
  "name": "react-static-starter-kit",
  "version": "9.0.0",
  "description": "Experimental React static prerendered app starter kit",
  "homepage": "https://react-static-starter-kit.netlify.app",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "prebuild": "npm run lint",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --profile --config config/build.conf.ts",
    "postbuild": "react-snap",
    "test": "jest -b --colors --passWithNoTests --testEnvironment=jsdom tests/$npm_config_files",
    "test:ts": "jest -b --colors --passWithNoTests --testEnvironment=jsdom src/$npm_config_files",
    "dev": "cross-env NODE_ENV=development webpack-cli serve --mode development --config config/build.conf.ts",
    "lint": "eslint --ext .ts --ext .tsx src config",
    "lint:fix": "npm run lint -- --fix"
  },
  "reactSnap": {
    "source": "build",
    "include": [
      "/404.html"
    ],
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "skipThirdPartyRequests": false
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/core-js": "^2.5.5",
    "@types/debug": "^4.1.7",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.19",
    "@types/node-polyglot": "^2.4.2",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "@types/require-dir": "^1.0.2",
    "@types/styled-components": "^5.1.23",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^2.0.5",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "debug": "^4.3.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "raw-loader": "^4.0.2",
    "react-refresh": "^0.11.0",
    "react-snap": "^1.23.0",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "speed-measure-webpack-plugin": "^1.5.0",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.69.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "abortcontroller-polyfill": "^1.7.3",
    "core-js": "^3.21.1",
    "lodash": "^4.17.21",
    "node-polyglot": "^2.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.5",
    "styled-components": "5.3.3",
    "styled-normalize": "^8.0.7",
    "whatwg-fetch": "^3.6.2"
  }
}
