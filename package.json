{
  "name": "react-static-starter-kit",
  "version": "16.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "http-server build -a localhost -P http://localhost:${PORT:-8080}? -p ${PORT:-8080}",
    "clean": "rimraf build",
    "prebuild": "npm run lint",
    "build": "npm run clean && BUILD_NUMBER=$(git rev-parse --short HEAD) webpack --profile --config config/build.conf.ts",
    "dev": "cross-env NODE_ENV=development webpack-cli serve --mode development --config config/build.conf.ts",
    "test": "jest --config=config/test.conf.ts tests/$npm_config_files",
    "test:unit": "jest --config=config/test.conf.ts src/$npm_config_files",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "npm run lint:ts:fix && npm run lint:css:fix",
    "lint:ts": "eslint --ext .ts --ext .tsx --ignore-pattern *.d.ts config lib src tests",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint src/**/*.css",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@testing-library/react": "^14.1.0",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/debug": "^4.1.12",
    "@types/html-webpack-plugin": "^3.2.9",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-router-dom": "^5.3.3",
    "@types/sprintf-js": "^1.1.4",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.6.3",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "esbuild-loader": "^4.0.2",
    "eslint": "^8.53.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.5.3",
    "http-server": "^14.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.3.0",
    "react-refresh": "^0.14.0",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "stylelint": "^15.11.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "whatwg-fetch": "^3.6.19",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.18.0",
    "react-router-dom": "^6.18.0",
    "sprintf-js": "^1.1.3"
  }
}
